int main(void) {
  int i, N;
  EncDouble a_, b_, res_, dummy_;
  i = 0;
  N = 10;
  a_ = 0.0; // encrypted
  b_ = 8.0; // encrypted
  res_ = 0.0;
  dummy_ = 0.0;
  dummy_ += 3.0; // Needed for Lattigo

  tstart();
  for (i = 0; i < N; i++) {
    res_ = a_ + b_;
  }
  tstop();
  print(res_);

  tstart();
  for (i = 0; i < N; i++) {
    res_ = a_ * b_;
  }
  tstop();
  print(res_);

  return 0;
}
